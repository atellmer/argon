{
  "name": "argon",
  "version": "1.0.0",
  "description": "Seeneco rendering engine for javaScript applications",
  "author": "Seeneco",
  "main": "./lib/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config webpack.dev.config",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev:watch": "webpack --config webpack.build.config --env.development --watch",
    "build:dev": "webpack --config webpack.build.config --env.development",
    "build:prod": "webpack --config webpack.build.config --env.production",
    "test": "npx ts-node-dev --transpileOnly ./node_modules/tape/bin/tape ./src/**/*.spec.ts | tap-notify | tap-difflet",
    "test:watch": "npx ts-node-dev --respawn --transpileOnly ./node_modules/tape/bin/tape ./src/**/*.spec.ts | tap-difflet",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "keywords": [],
  "devDependencies": {
    "@types/tape": "^4.2.33",
    "husky": "^0.13.1",
    "jsdom": "^15.0.0",
    "jsdom-global": "^3.0.2",
    "jsdom-simulant": "^1.1.2",
    "tap-difflet": "^0.7.1",
    "tap-notify": "^1.0.0",
    "tape": "^4.10.2",
    "tape-watch": "^2.3.0",
    "ts-loader": "^5.3.2",
    "ts-node": "^8.2.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.5.1",
    "webpack": "4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {}
}
